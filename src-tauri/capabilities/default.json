  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "sql:default",
    "sql:allow-execute",
    "fs:default",
    "core:webview:allow-create-webview-window",
    "core:window:allow-set-title",
