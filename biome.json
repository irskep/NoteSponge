{
  "formatter": {
    "indentStyle": "space",
    "indentWidth": 2,
    "lineWidth": 120,
    "lineEnding": "lf"
  },
  "linter": {
    "rules": {
      "a11y": {
        "recommended": true,
        "noAutofocus": "off",
        "useSemanticElements": "off"
      }
    }
  },
  "organizeImports": {
    "enabled": true
  }
}
